{
  "url": "https://api.stacksdata.info/v1/sql",
  "method": "POST",
  "table": "single_tx_blocks",
  "query": [
    "with rows as(",
    "    select",
    "        count(tx_id) tx_count, block_height",
    "    from",
    "        txs",
    "    WHERE",
    "        block_height >= get_max_block() - 10",
    "    group by",
    "        block_height",
    "    having",
    "        count(tx_id) < 2",
    "    order by",
    "        block_height",
    "    desc",
    ")",
    "SELECT",
    "  count(*)",
    "as",
    "  data", 
    "FROM", 
    "  rows"
  ]
}
