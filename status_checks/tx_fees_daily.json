{
  "url": "https://api.stacksdata.info/v1/sql",
  "method": "POST",
  "table": "tx_fees_daily",
  "query": [
    "select",
    "    round((avg(fee_rate) / 1000000), 4)",
    "as",
    "    data",
    "from",
    "    txs",
    "where",
    "    block_time::Date>=current_date-1",
    "order by 1"
  ]
}
