{
  "env": {
    "browser": true,
    "es2021": true,
  },
  "extends": [
    "plugin:react/recommended",
    "standard",
    "eslint:recommended",
    "next",
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect",
    },
  },
  "rules": {
    "react/prop-types": 0,
    "no-extra-semi": 0,
    "semi": 0,
    "quotes": "off",
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "no-var": "off",
  },
}
